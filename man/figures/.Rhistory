library("ggplot2")
library("scales")
df.btc <- read.csv("http://192.168.0.3/api/btc/btc-data.txt")
df.btc.m <- df.btc %>%
group_by(Date) %>%
summarise(fiat = sum(Fiat..EUR., na.rm = TRUE),
btc = sum(On.Chain.Balance..BTC., na.rm=TRUE))
df.btc.m$Date <- as.POSIXct(df.btc.m$Date)
d <- as.POSIXct(as.character(Sys.time()), format="%Y-%m-%d")
date <- as.Date(format(d, format = "%Y-%m-%d"))
df.btc.m.30 <- subset(df.btc.m, Date > date-30)
coeff = max(df.btc.m.30$fiat)/max(df.btc.m.30$btc)
ggplot() +
geom_line(df.btc.m.30, mapping= aes(Date, as.numeric(btc)), colour="#E53935", size=1) +
geom_line(df.btc.m.30, mapping= aes(Date, as.numeric(fiat)/coeff), linetype=2, size=0.5) +
geom_smooth(df.btc.m.30, mapping=aes(Date, as.numeric(fiat)/coeff), method="loess", colour="#304FFE", size=1) +
theme_bw() +
theme(axis.title.x = element_blank(),
axis.title.y.left = element_text(colour="#E53935"),
axis.title.y.right = element_text(colour="#304FFE")) +
scale_y_continuous(name = "On-Chain Balance (BTC)",
sec.axis = sec_axis(trans=~.*coeff, name="Fiat (EUR)")) +
scale_x_datetime(labels = date_format("%d.%m.%y\n%H:%M"), expand=c(0,0), date_breaks = "5 days") +
ggtitle("On-Chain Balance (last 30 days)")
