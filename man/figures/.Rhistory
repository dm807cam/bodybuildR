library("gmp")
library("tidyverse")
library("ggplot2")
primary <- '192.168.0.3'
machineAddresses <- list(
list(host=primary,user='dmayk',
ncore=4),
list(host='192.168.0.250',user='umbrel',
ncore=4)
)
spec <- lapply(machineAddresses,
function(machine) {
rep(list(list(host=machine$host,
user=machine$user)),
machine$ncore)
})
spec <- unlist(spec,recursive=FALSE)
parallelCluster <- parallel::makeCluster(type='PSOCK',
master=primary,
spec=spec)
