library("scales")
df.btc <- read.csv("http://192.168.0.3/api/btc/btc-data.txt")
df.btc.m <- df.btc %>%
group_by(Date) %>%
summarise(fiat = sum(Fiat..EUR., na.rm = TRUE),
btc = sum(On.Chain.Balance..BTC., na.rm=TRUE))
df.btc.m$Date <- as.POSIXct(df.btc.m$Date)
coeff = max(df.btc.m$fiat)/max(df.btc.m$btc)
ggplot() +
geom_line(df.btc.m, mapping= aes(Date, as.numeric(btc)), colour="#E53935", size=1) +
geom_line(df.btc.m, mapping= aes(Date, as.numeric(fiat)/coeff), linetype=2, size=0.5) +
geom_smooth(df.btc.m, mapping=aes(Date, as.numeric(fiat)/coeff), method="loess", colour="#304FFE", size=1) +
theme_bw() +
theme(axis.title.x = element_blank(),
axis.title.y.left = element_text(colour="#E53935"),
axis.title.y.right = element_text(colour="#304FFE")) +
scale_y_continuous(name = "On-Chain Balance (BTC)",
sec.axis = sec_axis(trans=~.*coeff, name="Fiat (EUR)"),
limit=c(min(df.btc.m$btc)-0.05*min(df.btc.m$btc),
max(df.btc.m$btc)+0.01*max(df.btc.m$btc))) +
scale_x_datetime(labels = date_format("%d.%m.%y\n%H:%M"), expand=c(0,0))
